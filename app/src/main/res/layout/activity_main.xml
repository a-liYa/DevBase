<?xml version="1.0" encoding="utf-8"?>
<com.aliya.base.sample.ui.widget.WBFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_frame"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aliya.base.sample.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.aliya.view.fitsys.FitWindowsFrameLayout
            android:id="@+id/frame_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@android:color/white" />

        <com.aliya.base.sample.ui.MainTabLayout
            android:id="@+id/tab_layout"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="#f5f5f5" />
    </LinearLayout>
</com.aliya.base.sample.ui.widget.WBFrameLayout>